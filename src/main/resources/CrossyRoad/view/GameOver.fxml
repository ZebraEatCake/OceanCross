<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CrossyRoad.view.GameOverController">
    <children>
        <Label layoutX="123.0" layoutY="150.0" text="Game Over!">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
        <Button fx:id="restartButton" layoutX="150.0" layoutY="250.0" mnemonicParsing="false" onAction="#handleRestart" text="Restart Game">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
    </children>
</AnchorPane>